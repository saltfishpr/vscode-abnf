{
    "comments": {
        "lineComment": ";"
    },
    "brackets": [
        [
            "(",
            ")"
        ],
        [
            "[",
            "]"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "<",
            "close": ">"
        }
    ],
    "surroundingPairs": [
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "<",
            "close": ">"
        }
    ],
    "folding": {
        "markers": {
            "start": "^[A-Za-z]",
            "end": "^(?=[A-Za-z]|\\s*$)"
        }
    },
    "indentationRules": {
        "increaseIndentPattern": "^[A-Za-z][A-Za-z0-9\\-]*\\s*=\\/?",
        "decreaseIndentPattern": "^[A-Za-z]"
    },
    "wordPattern": "[A-Za-z][A-Za-z0-9\\-]*"
}